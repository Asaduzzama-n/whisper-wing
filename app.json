{
  "expo": {
    "name": "whisper-wing",
    "slug": "whisper-wing",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#CCB8E0"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#CCB8E0"
    },
    "androidStatusBar": {
      "backgroundColor": "#CCB8E0",
      "barStyle": "light-content"
    },
    "androidNavigationBar": {
      "backgroundColor": "#CCB8E0"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#CCB8E0"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
